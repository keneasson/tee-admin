{
  "name": "tee-admin",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "engines": {
    "node": "22.17.0",
    "npm": "please-use-yarn",
    "yarn": "4.1.0"
  },
  "scripts": {
    "native": "cd apps/expo && yarn start",
    "watch": "yarn workspaces foreach --all -pi run watch",
    "vercel:install": "yarn set version 4.0.2 && yarn install",
    "web": "yarn build && cd apps/next && yarn next",
    "web:extract": "DISABLE_EXTRACTION=false yarn workspace next-app dev",
    "web:prod": "yarn workspace next-app build",
    "web:prod:serve": "yarn workspace next-app serve",
    "email": "yarn build && cd apps/email-builder && yarn dev --dir=emails",
    "fix": "manypkg fix",
    "postinstall": "yarn check-deps && yarn build",
    "build": "yarn workspaces foreach --all --exclude next-app run build",
    "upgrade:tamagui": "yarn up '*tamagui*'@latest '@tamagui/*'@latest react-native-web-lite@latest",
    "upgrade:tamagui:canary": "yarn up '*tamagui*'@canary '@tamagui/*'@canary react-native-web-lite@canary",
    "check-deps": "check-dependency-version-consistency .",
    "test": "cd apps/next && npm test",
    "test:auth": "cd apps/next && npm test tests/auth/role-assignment-simple.test.ts",
    "test:e2e": "cd apps/next && npm run test:e2e",
    "fix-auth": "bash scripts/test-and-fix.sh"
  },
  "resolutions": {
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-refresh": "^0.14.0",
    "react-native-svg": "15.3.0",
    "react-native-web": "^0.20.0"
  },
  "dependencies": {
    "@babel/runtime": "^7.26.9",
    "@manypkg/cli": "^0.23.0",
    "check-dependency-version-consistency": "^5.0.0",
    "eslint": "^9.3.0",
    "node-gyp": "^11.1.0",
    "prettier": "^3.5.2",
    "turbo": "^2.4.2",
    "typescript": "^5.8.3"
  },
  "packageManager": "yarn@4.6.0",
  "devDependencies": {
    "@types/bcryptjs": "^3.0.0",
    "@vitest/ui": "^3.2.4",
    "vitest": "^3.2.4"
  }
}
