{
  "name": "tee-admin",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "engines": {
    "node": "22.17.0",
    "npm": "please-use-yarn",
    "yarn": "4.6.0"
  },
  "scripts": {
    "native": "cd apps/expo && yarn start",
    "watch": "yarn workspaces foreach --all -pi run watch",
    "vercel:install": "yarn set version 4.6.0 && yarn install",
    "web": "yarn build && cd apps/next && yarn next -p 4000",
    "web:extract": "DISABLE_EXTRACTION=false yarn workspace next-app dev",
    "web:prod": "yarn workspace next-app build",
    "web:prod:serve": "yarn workspace next-app serve",
    "email": "yarn build && cd apps/email-builder && yarn dev --dir=emails",
    "fix": "manypkg fix",
    "postinstall": "yarn check-deps && yarn build",
    "build": "yarn workspaces foreach --all --exclude next-app run build",
    "build:check": "yarn workspace next-app build:check",
    "upgrade:tamagui": "yarn up '*tamagui*'@latest '@tamagui/*'@latest react-native-web-lite@latest",
    "upgrade:tamagui:canary": "yarn up '*tamagui*'@canary '@tamagui/*'@canary react-native-web-lite@canary",
    "check-deps": "check-dependency-version-consistency .",
    "test": "cd apps/next && npm test",
    "test:auth": "cd apps/next && npm test tests/auth/role-assignment-simple.test.ts",
    "test:e2e": "cd apps/next && npm run test:e2e",
    "fix-auth": "bash scripts/test-and-fix.sh",
    "setup-dynamodb": "tsx scripts/setup-dynamodb.ts create",
    "setup-dynamodb:dev": "STAGE=dev tsx scripts/setup-dynamodb.ts dev",
    "setup-dynamodb:delete": "tsx scripts/setup-dynamodb.ts delete",
    "list-tables": "tsx scripts/list-dynamodb-tables.ts",
    "test-webhook": "cd apps/next && tsx ../../scripts/test-webhook-system.ts all",
    "test-webhook:health": "cd apps/next && tsx ../../scripts/test-webhook-system.ts health",
    "test-webhook:sheets": "cd apps/next && tsx ../../scripts/test-webhook-system.ts sheets",
    "test-webhook:security": "cd apps/next && tsx ../../scripts/test-webhook-system.ts security",
    "test-webhook:sync": "cd apps/next && tsx ../../scripts/test-webhook-system.ts sync",
    "test-api-migration": "cd apps/next && tsx ../../scripts/test-api-migration.ts all",
    "test-api-migration:health": "cd apps/next && tsx ../../scripts/test-api-migration.ts health",
    "test-api-migration:schedule": "cd apps/next && tsx ../../scripts/test-api-migration.ts schedule",
    "test-api-migration:directory": "cd apps/next && tsx ../../scripts/test-api-migration.ts directory",
    "test-api-migration:upcoming": "cd apps/next && tsx ../../scripts/test-api-migration.ts upcoming",
    "populate-dynamodb": "tsx scripts/populate-dynamodb.ts",
    "validate-data": "cd apps/next && tsx ../../scripts/validate-data.ts",
    "test-newsletter-timing": "cd apps/next && tsx ../../scripts/test-newsletter-timing.ts"
  },
  "resolutions": {
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-refresh": "^0.14.0",
    "react-native-svg": "15.3.0",
    "react-native-web": "^0.20.0"
  },
  "packageManager": "yarn@4.6.0",
  "devDependencies": {
    "@babel/runtime": "^7.26.9",
    "@manypkg/cli": "^0.23.0",
    "@types/bcryptjs": "^3.0.0",
    "@vitest/ui": "^3.2.4",
    "check-dependency-version-consistency": "^5.0.0",
    "eslint": "^9.3.0",
    "node-gyp": "^11.1.0",
    "prettier": "^3.5.2",
    "tsx": "^4.19.2",
    "turbo": "^2.4.2",
    "typescript": "^5.8.3",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "@tanstack/react-table": "^8.21.3"
  }
}
